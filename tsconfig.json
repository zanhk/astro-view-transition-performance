{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"strictNullChecks": true,
		"baseUrl": ".",
		"paths": {
			"@/*": ["src/*"]
		},
		"types": [
			"astro/client",
			"vite-plugin-pwa/vanillajs",
			"vite-plugin-pwa/info",
			"vite-plugin-pwa/pwa-assets"
		]
	},
	"include": [".", "src/**/*", ".keystatic/**/*"]
}
